# Copy this to .env and fill in your values.

# Supabase (client-side)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Postgres (Prisma uses this for migrations)
# Use the *direct* connection host on port 5432.
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@db.your-id.supabase.co:5432/postgres?sslmode=require
SHADOW_DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@db.your-id.supabase.co:5432/postgres?sslmode=require
